# ================================
# Node.js / Express.js .gitignore
# ================================

# Dependencies
node_modules/

# Environment variables
.env
.env.local
.env.development
.env.production
.env.test

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage
coverage/
.nyc_output/

# Build output (if any)
dist/
build/

# Temporary files
tmp/
temp/

# OS files
.DS_Store
Thumbs.db

# Editor / IDE settings
.vscode/
.idea/
*.swp

# Uploads (optional - ignore if using local storage)
uploads/
public/uploads/

# Cache
.cache/
.eslintcache

# Testing
jest.config.js
